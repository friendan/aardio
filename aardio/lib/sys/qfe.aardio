//qfe 系统补丁
import com.wmi;

namespace sys;

class qfe{
	ctor(hotfixId){
		return ..com.wmi.getProperties("SELECT * FROM Win32_QuickFixEngineering WHERE HotFixID =@hotfixId",{hotfixId=hotfixId} ) 
	}; 
}

