//FileInput
import win.ui;
/*DSG{{*/
var winform = win.form(text="aardio form";right=759;bottom=469)
winform.add()
/*}}*/

import web.view;
var wb = web.view(winform);

wb.html = /**
<!doctype html>
<html>
<head> 
</head>
<body>
     <input type="file">
</body>
</html>
**/

//获取控件
var file = wb.cdpWaitQuery(`input[type="file"]`);

//设置文件路径
wb.cdp("DOM.setFileInputFiles",{
	files = { io._exepath };
	nodeId = file.nodeId;
})

winform.show();
win.loopMessage();
